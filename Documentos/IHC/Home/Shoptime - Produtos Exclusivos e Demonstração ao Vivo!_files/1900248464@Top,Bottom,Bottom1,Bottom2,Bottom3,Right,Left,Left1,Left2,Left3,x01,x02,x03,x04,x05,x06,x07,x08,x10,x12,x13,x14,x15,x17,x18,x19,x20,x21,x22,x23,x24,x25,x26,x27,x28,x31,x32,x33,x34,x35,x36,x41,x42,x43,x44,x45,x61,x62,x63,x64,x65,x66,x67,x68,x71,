document.write ('        // Expressao regular para email\n');
document.write ('        var emailReg = /^([\\');
document.write ('w-\\');
document.write ('.]+@([\\');
document.write ('w-]+\\');
document.write ('.)+[\\');
document.write ('w-]{2,4})?$/;\n');
document.write ('\n');
document.write ('        // Retorna true caso esteja correto\n');
document.write ('        return (email !== "" && emailReg.test(email));\n');
document.write ('    }\n');
document.write ('});\n');
